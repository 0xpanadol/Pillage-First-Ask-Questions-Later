{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["dom", "dom.iterable", "esnext", "webworker"],
    "types": ["vite/client", "vite-plugin-svgr/client", "jest"],
    "baseUrl": "./",
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "module": "esnext",
    "paths": {
      "app/*": [
        "src/app/*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "database/*": [
        "src/database/*"
      ],
      "utils/*": [
        "src/utils/*"
      ],
      "hooks/*": [
        "src/hooks/*"
      ],
      "constants/*": [
        "src/constants/*"
      ],
      "maps/*": [
        "src/maps/*"
      ],
      "i18n/*": [
        "src/i18n/*"
      ],
      "interfaces/*": [
        "src/interfaces/*"
      ],
      "providers/*": [
        "src/providers/*"
      ],
      "services/*": [
        "src/services/*"
      ],
      "styles/*": [
        "src/styles/*"
      ],
      "assets/*": [
        "src/assets/*"
      ],
      "config/*": [
        "config/*"
      ],
      "mocks/*": [
        "__mocks__/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "vite-env.d.ts",
    "src",
    "config",
    "**.config.ts",
    "__mocks__"
  ]
}
